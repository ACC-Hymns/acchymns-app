<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ChristopherW.acchmns" version="1.3.0" android-versionCode="11300" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ACC Hymns</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/ios/icon_180.png" />

    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/android/xxhdpi.png" density="xxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="scheme" value="app" />
        <preference name="hostname" value="localhost" />
        <!-- iOS 8.0+ -->
        <!-- iPhone 6 Plus  -->
        <icon src="res/ios/icon_180.png" width="180" height="180" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="res/ios/icon_60.png" width="60" height="60" />
        <icon src="res/ios/icon_120.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="res/ios/icon_76.png" width="76" height="76" />
        <icon src="res/ios/icon_152.png" width="152" height="152" />
        <!-- Spotlight Icon -->
        <icon src="res/ios/icon_40.png" width="40" height="40" />
        <icon src="res/ios/icon_80.png" width="80" height="80" />
        <!-- iOS 6.1 -->
        <!-- iPhone / iPod Touch -->
        <icon src="res/ios/icon_58.png" width="58" height="58" />
        <icon src="res/ios/icon_120.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="res/ios/icon_76.png" width="76" height="76" />
        <icon src="res/ios/icon_152.png" width="152" height="152" />
        <!-- iPad Pro -->
        <icon src="res/ios/icon_167.png" width="167" height="167" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="res/ios/icon_29.png" width="29" height="29" />
        <icon src="res/ios/icon_58.png" width="58" height="58" />
        <icon src="res/ios/icon_87.png" width="87" height="87" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="res/ios/icon_58.png" width="58" height="58" />
        <icon src="res/ios/icon_120.png" width="120" height="120" />
        <!-- iTunes Marketing Image -->
        <icon src="res/ios/icon_1024.png" width="1024" height="1024" />
        <config-file parent="CFBundleDisplayName" platform="ios" target="*-Info.plist">
            <string>Hymnal</string>
        </config-file>
    </platform>
</widget>
